{
    "name": "savvy/pages-bundle",
    "type": "symfony-bundle",
    "description": "Pages bundle for use with savvy cms built in Symfony2",
    "keywords": ["cms", "bundle", "pages"],
    "homepage": "http://github.com/oxygenthinking/pages-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Rotherfield",
            "email": "luke@savvycreativeuk.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<=2.2-dev",
        "knplabs/knp-menu":         "2.0.*",
        "knplabs/knp-menu-bundle": "2.0.x-dev"
    },
    "autoload": {
        "psr-0": { "Savvy\\PagesBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Savvy/PagesBundle"
}
